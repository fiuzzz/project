


<div class="row">
    <div class="col-md-12">
      <div class="card">
        <div class="table-responsive pt-3">
          <table class="table table-striped project-orders-table">
            <thead>
              <tr>
                <th>ID</th>
                <th>Genre</th>
                <th>Jumlah Komik</th>

              </tr>
            </thead>
            <tbody>
              <% genre.forEach(element => { %>
               <tr>
                <td><%-element.id  %></td>
                <td><%-element.genre  %></td>
                <td><%-element.jumlah_comic  %></td>
               </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>


